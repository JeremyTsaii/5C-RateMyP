{
    "name": "5C RateMyP",
    "version": "4.0.0",
    "description": "Conveniently view ratings of 5C professors while crafting your schedule!",
    "author": "Jeremy T.",
    "permissions": ["activeTab", "storage"],
    "content_scripts": [
        {
            "matches": ["http://hyperschedule.io/*", "https://hyperschedule.io/*"],
            "css": ["main.css"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        "main.css"
    ],
    "page_action": {
        "default_title": "View 5C professors' ratings",
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
            }
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"        
    },
    "manifest_version": 2

}